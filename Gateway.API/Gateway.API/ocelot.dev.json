{
  "Routes": [
    //Dashboard Controller APIs
    {
      "DownstreamPathTemplate": "/api/Dashboard/{catchAll}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/gateway/Dashboard/{catchAll}",
      "UpstreamHttpMethod": [ "Get" ]

     },
    //ExpenseTypes Controller APIs
            //ExpenseTypes Controller for Get , Post , Put
    {
      "DownstreamPathTemplate": "/api/ExpenseTypes/{catchAll}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/gateway/Dashboard/{catchAll}/{id}",
      "UpstreamHttpMethod": [ "Get", "POST", "PUT" ]

    }
         //ExpenseTypes Controller for Get , Post , Put

  ],
  //Global Configuration for this Ocelot project 
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:5021"
  }
}
