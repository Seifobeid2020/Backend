{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/Dashboard/{catchAll}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/gateway/Dashboard/{catchAll}",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/api/ExpenseTypes/{catchAll}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/gateway/Dashboard/{catchAll}/{id}",
      "UpstreamHttpMethod": [ "Get", "POST", "PUT" ]
    },
    //Patient Controller Handler
    //Get , POST
    {
      "DownstreamPathTemplate": "/api/Patients",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/gateway/Patients",
      "UpstreamHttpMethod": [ "Get", "POST" ]
    },
    //Get(id) , put(id) , Delete(id)
    {
      "DownstreamPathTemplate": "/api/Patients/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/gateway/Patients/{id}",
      "UpstreamHttpMethod": [ "Delete", "get", "put" ]
