    {
      "DownstreamPathTemplate": "/api/expensetypes/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/gateway/ExpenseTypes/{id}",
      "UpstreamHttpMethod": [ "Delete", "get", "put" ]
